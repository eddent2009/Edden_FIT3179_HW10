{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Fluoridation Levels by Suburb in Victoria",
  "width": 800,
  "height": 600,
  "projection": {
    "type": "mercator",
    "center": [144.9631, -37.8136],
    "scale": 4000
  },
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/eddent2009/Edden_FIT3179_HW9/refs/heads/main/js/vic_localities.json",
        "format": {"type": "topojson", "feature": "vic_localities"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/eddent2009/Edden_FIT3179_HW9/refs/heads/main/js/ne_110m_graticules_1.json",
        "format": {"type": "topojson", "feature": "ne_110m_graticules_1"}
      },
      "mark": {"type": "geoshape", "fill": null, "stroke": "lightgray"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/eddent2009/Edden_FIT3179_HW9/refs/heads/main/data/Fluoridation%20by%20postcode%20-%20Sheet1.csv",
        "format": {"type": "csv"}
      },
      "mark": "circle",
      "encoding": {
        "longitude": {"field": "lon", "type": "quantitative"},
        "latitude": {"field": "lat", "type": "quantitative"},
        "size": {"value": 50},
        "color": {
          "field": "fluoride_level",
          "type": "nominal",
          "scale": {"scheme": "category10"}
        },
        "tooltip": [
          {"field": "townsuburb", "type": "nominal", "title": "Suburb"},
          {
            "field": "fluoride_level",
            "type": "nominal",
            "title": "Fluoridation Level"
          }
        ]
      }
    }
  ],
  "config": {}
}